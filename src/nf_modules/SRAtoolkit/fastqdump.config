profiles {
  docker {
    docker.temp = 'auto'
    docker.enabled = true
    process {
      withname: fastq_dump {
        container = "sratoolkit:2.8.2"
      }
    }
  }
  psmn {
    process{
      withname: fastq_dump {
        beforeScript = "source /usr/share/lmod/lmod/init/bash; module use ~/privatemodules"
        module = "SRAtoolkit/2.8.2"
        executor = "sge"
        clusterOptions = "-m e -cwd -V"
        memory = "20GB"
        time = "12h"
        queue = 'monointeldeb128'
      }
    }
  }
}
