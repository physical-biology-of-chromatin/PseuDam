profiles {
  docker {
    docker.temp = 'auto'
    docker.enabled = true
    process {
      withname: fastqc_fastq {
        container = "fastqc:0.11.5"
      }
    }
  }
  psmn {
    process{
      withname: fastqc_fastq {
        beforeScript = "source /usr/share/lmod/lmod/init/bash; module use ~/privatemodules"
        module = "FastQC/0.11.5"
        executor = "sge"
        clusterOptions = "-m e -cwd -V"
        memory = "20GB"
        time = "12h"
        queue = 'monointeldeb128'
      }
    }
  }
}
