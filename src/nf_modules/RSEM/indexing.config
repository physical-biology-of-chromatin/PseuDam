profiles {
  docker {
    docker.temp = 'auto'
    docker.enabled = true
    process {
      $index_fasta {
        container = "rsem:1.3.0"
      }
    }
  }
  sge {
    process{
      $index_fasta {
        beforeScript = "module purge; module load RSEM/1.3.0; module load SAMtools/1.7"
      }
    }
  }
}
